APP_NAME=
APP_ENV=
APP_KEY=
APP_DEBUG=
APP_URL=

# Channel Lister Configuration
CHANNEL_LISTER_ENABLED=
CHANNEL_LISTER_DOMAIN=
CHANNEL_LISTER_PATH=
CHANNEL_LISTER_API_PATH=
CHANNEL_LISTER_DEFAULT_WAREHOUSE=

# Amazon Caching Configuration
# Disk to use for schema caching (local, s3, etc.)
CHANNEL_LISTER_AMAZON_CACHE_DISK=local

# Cache TTL in seconds
CHANNEL_LISTER_AMAZON_CACHE_TTL_PRODUCT_SEARCH=3600    # 1 hour
CHANNEL_LISTER_AMAZON_CACHE_TTL_REQUIREMENTS=86400     # 24 hours
CHANNEL_LISTER_AMAZON_CACHE_TTL_SCHEMA=604800          # 7 days

# Schema cache directory path
CHANNEL_LISTER_AMAZON_CACHE_SCHEMA_PATH=amazon-schemas

# ShipStation API Configuration
# Set CHANNEL_LISTER_SHIPSTATION_API_KEY to enable automatic shipping calculations
# If not set, users will need to enter shipping costs manually
CHANNEL_LISTER_SHIPSTATION_API_KEY=
CHANNEL_LISTER_SHIPSTATION_BASE_URL=
